{
  "name": "bcb-intetest-check",
  "version": "1.0.0",
  "license": "MIT",
  "main": "electron/main.js",
  "author": {
    "name": "Ruan Frohlich",
    "email": "ruanfrohlich@gmail.com"
  },
  "description": "Programa para verificar taxa de  juros abusiva.",
  "targets": {
    "default": {
      "publicUrl": "."
    },
    "main": false,
    "electron": {
      "distDir": "electron",
      "engines": {
        "node": ">=22"
      },
      "source": [
        "src/main.ts",
        "src/preload.ts"
      ]
    },
    "static": {
      "distDir": "electron/static",
      "publicUrl": ".",
      "source": [
        "src/index.html",
        "src/renderer.ts"
      ]
    }
  },
  "build": {
    "appId": "com.ruanfrohlich.BCBInterestCheck",
    "productName": "BCB Interest Check",
    "artifactName": "BCBInterestCheck.${ext}",
    "icon": "./images/logo.ico"
  },
  "scripts": {
    "dev": "nodemon --watch src -e js,ts,html -x 'yarn start'",
    "start": "yarn build && electron .",
    "build": "rimraf -rf electron && parcel build && electron-builder -w --x64"
  },
  "dependencies": {
    "@puppeteer/browsers": "^2.7.0",
    "cheerio": "^1.0.0",
    "lodash": "^4.17.21",
    "puppeteer": "^24.1.1",
    "puppeteer-core": "^24.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.12.0",
    "dotenv": "^16.4.7",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-builder-squirrel-windows": "^25.1.8",
    "http-status-codes": "^2.3.0",
    "nodemon": "^3.1.9",
    "parcel": "^2.13.3",
    "path-browserify": "^1.0.0",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}