{
  "name": "bcb-intetest-check",
  "version": "1.0.0",
  "license": "MIT",
  "main": "electron/main.js",
  "author": {
    "name": "Ruan Frohlich",
    "email": "ruanfrohlich@gmail.com"
  },
  "description": "Programa para verificar taxa de  juros abusiva.",
  "targets": {
    "main": false,
    "electron": {
      "distDir": "electron",
      "engines": {
        "node": ">=22"
      },
      "source": [
        "src/main.ts",
        "src/preload.ts"
      ]
    },
    "static": {
      "distDir": "electron/static",
      "publicUrl": ".",
      "source": [
        "src/views/index.html"
      ]
    }
  },
  "build": {
    "appId": "com.ruanfrohlich.BCBInterestCheck",
    "productName": "BCB Interest Check",
    "artifactName": "BCBInterestCheck.${ext}",
    "icon": "./src/images/logo.ico"
  },
  "scripts": {
    "dev": "nodemon -w src -e js,ts,tsx,html,css -x 'parcel watch & electron .'",
    "start": "rimraf -rf electron && parcel build && electron .",
    "build": "rimraf -rf electron && parcel build && electron-builder -wl"
  },
  "dependencies": {},
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.3",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-builder-squirrel-windows": "^25.1.8",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.9",
    "parcel": "^2.13.3",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "puppeteer": "^24.1.1",
    "puppeteer-core": "^24.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}